#the goal here is just to see if the DNN will train beyond random-guessing.
test_iter: 100
test_interval: 500
base_lr: 0.01
display: 40
max_iter: 10000
lr_policy: "step"
gamma: 0.1
momentum: 0.9
weight_decay: 0.0002
snapshot: 1000
snapshot_prefix: "train"
solver_mode: GPU
random_seed: 42
net: "trainval.prototxt"
test_initialization: false
average_loss: 40
